{
  "name": "react-router-hook",
  "version": "2.0.0-beta.4",
  "description": "Universal data fetching and lifecycle management for react router with multiple components",
  "source": "src/index.js",
  "module": "dist/react-router-hook.es.js",
  "main": "dist/react-router-hook.js",
  "umd:main": "dist/react-router-hook.umd.js",
  "scripts": {
    "clean": "rimraf dist",
    "build:commonjs": "cross-env NODE_ENV=cjs rollup -c -f cjs -o dist/react-router-hook.js",
    "build:es": "cross-env BABEL_ENV=es NODE_ENV=es rollup -c -f es -o dist/react-router-hook.es.js",
    "build:umd": "cross-env BABEL_ENV=es NODE_ENV=development rollup -c -f umd -o dist/react-router-hook.umd.js",
    "build:umd:min": "cross-env BABEL_ENV=es NODE_ENV=production rollup -c -f umd -o dist/react-router-hook.umd.min.js",
    "build": "npm-run-all -p build:commonjs build:es build:umd build:umd:min",
    "lint": "eslint src test",
    "posttest": "npm run lint",
    "prepare": "npm run clean && npm run build",
    "test": "jest",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls"
  },
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kouhin/react-router-hook.git"
  },
  "keywords": [
    "react",
    "react-router",
    "hook",
    "async",
    "fetch",
    "react-component",
    "reactjs"
  ],
  "author": [
    "Bin Hou <houbin217jz@gmail.com> (https://twitter.com/houbin217jz)"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.47",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.47",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.47",
    "@babel/preset-env": "^7.0.0-beta.47",
    "@babel/preset-react": "^7.0.0-beta.47",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.3",
    "chai": "^4.1.2",
    "coveralls": "^3.0.1",
    "cross-env": "^5.1.5",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "17.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.18.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.9.1",
    "jest": "^23.0.0-charlie.2",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.12.1",
    "react": "^16.3.2",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.63.2",
    "rollup-plugin-auto-external": "^1.2.0",
    "rollup-plugin-babel": "^4.0.0-beta.4",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^4.0.0"
  },
  "dependencies": {
    "create-react-context": "^0.2.2",
    "dlv": "^1.1.2",
    "hoist-non-react-statics": "^2.5.5",
    "unistore": "^3.0.6",
    "uuid": "^3.3.2"
  },
  "peerDependencies": {
    "react": "*"
  }
}
